{
  "linea1": { "texto": "La lucha ha comenzado", "fonetica": "la ˈlutʃa a komenˈsaðo" },
  "linea2": { "texto": "¿Quién será el vencedor?", "fonetica": "kjen seˈɾa el βenθeˈðoɾ" },
  "linea3": { "texto": "Un corazón ardiente apunta al gol", "fonetica": "un koɾaˈθon aɾˈðjente aˈpunta al ɣol" },
  "linea4": { "texto": "Capitán Centella", "fonetica": "kapiˈtan θenˈteʎa" },
  "linea5": { "texto": "Corre cortando el viento", "fonetica": "ˈkore koɾˈtando el ˈβjento" },
  "linea6": { "texto": "El balón es mi alma", "fonetica": "el βaˈlon es mi ˈalma" },
  "linea7": { "texto": "Persiguiendo un sueño", "fonetica": "persiˈɣjendo un ˈsweɲo" },
  "linea8": { "texto": "Hoy también en la cancha", "fonetica": "oi tamˈbjen en la ˈkanʧa" },
  "linea9": { "texto": "Capitán Centella", "fonetica": "kapiˈtan θenˈteʎa" },
  "linea10": { "texto": "El tiro hacia la victoria", "fonetica": "el ˈtiɾo ˈasja la βikˈtoɾja" },
  "linea11": { "texto": "Voy a anotar", "fonetica": "ˈboi a anoˈtaɾ" },
  "linea12": { "texto": "Nuestro fútbol", "fonetica": "ˈnwestɾo ˈfuðβol" },
  "linea13": { "texto": "Capitán Centella", "fonetica": "kapiˈtan θenˈteʎa" }
}